<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8.0dip" />
            <gradient android:startColor="#ff4d4d4d" android:endColor="#ff4d4d4d" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff4d4d4d" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="8.0dip" />
                <gradient android:startColor="#80ffd300" android:endColor="#a0ffcb00" android:angle="270.0" android:centerY="0.75" android:centerColor="#80ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8.0dip" />
                <gradient android:startColor="#ff3f74b8" android:endColor="#ff3f74b8" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff5788be" />
            </shape>
        </clip>
    </item>
</layer-list>